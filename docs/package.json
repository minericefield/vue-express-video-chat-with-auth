  "description": "For swagger generating docs.",
  "private": true,
  "scripts": {
    "generate-client": "openapi-generator-cli  generate -g typescript-axios -i ./openapi.yml -o ../client/src/types/swagger/"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.1.19"
  },
